<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".searchCategory">

    <ImageButton
        android:id="@+id/Fruits"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="#FFFFFF"
        android:src="@drawable/fruits"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.017"
        app:layout_goneMarginTop="5dp"
        android:contentDescription="TODO" />

    <ImageButton
        android:id="@+id/Meat"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="#FFFFFF"
        android:src="@drawable/meat"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.016" />

    <ImageButton
        android:id="@+id/Vegetable"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="#FFFFFF"
        android:src="@drawable/vegetables"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Fruits" />

    <ImageButton
        android:id="@+id/Dairy"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="#FFFFFF"
        android:src="@drawable/dairy"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Meat" />

    <ImageButton
        android:id="@+id/Bakery"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="#FFFFFF"
        android:src="@drawable/bakery"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Vegetable" />


    <ImageButton
        android:id="@+id/Snack"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="#FFFFFF"
        android:src="@drawable/snack"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Dairy" />

    <ImageButton
        android:id="@+id/Liquor"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="#FFFFFF"
        android:src="@drawable/liquor"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Bakery" />

    <ImageButton
        android:id="@+id/Beverage"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="#FFFFFF"
        android:src="@drawable/beverages"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Snack" />

    <ImageButton
        android:id="@+id/Cleaning"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="#FFFFFF"
        android:src="@drawable/cleaning"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Liquor" />

    <ImageButton
        android:id="@+id/Hygiene"
        android:layout_width="200dp"
        android:layout_height="130dp"
        android:background="#FFFFFF"
        android:src="@drawable/hygiene"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Beverage" />

</androidx.constraintlayout.widget.ConstraintLayout>